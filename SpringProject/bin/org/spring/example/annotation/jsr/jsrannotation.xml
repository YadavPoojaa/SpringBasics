		<property name="pointA" ref="point1"></property>
		<property name="pointB" ref="point2"></property>
		<property name="pointC" ref="point3"></property>
	</bean>
	
	 -->
	
	<bean id="point1" class="org.spring.example.annotation.jsr.Point">
	<qualifier value="circlecenter"></qualifier>
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	
	 <bean id="circle" class="org.spring.example.annotation.jsr.Circle">
	
	</bean>
	 
	<bean id="point2" class="org.spring.example.annotation.jsr.Point">
		<property name="x" value="0"></property>
		<property name="y" value="-10"></property>
	</bean>
	
	<bean id="center" class="org.spring.example.annotation.jsr.Point">
		<property name="x" value="10"></property>
		<property name="y" value="0"></property>
	</bean>
	
	<context:annotation-config/>
</beans>