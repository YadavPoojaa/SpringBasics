<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="triangle" class="org.spring.example.autowiring.Triangle" autowire="byName">
	<!-- byType
	byConstructor -->
	</bean>
	
	<bean id="pointA" class="org.spring.example.autowiring.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	
	<bean id="pointB" class="org.spring.example.autowiring.Point">
		<property name="x" value="0"></property>
		<property name="y" value="-10"></property>
	</bean>
	
	<bean id="pointC" class="org.spring.example.autowiring.Point">
		<property name="x" value="10"></property>
		<property name="y" value="0"></property>
	</bean>
	
	
</beans>